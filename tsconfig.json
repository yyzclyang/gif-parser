{
  "compilerOptions": {
    "target": "es5",
    "useDefineForClassFields": true,
    "module": "esNext",
    "moduleResolution": "node",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "allowJs": true,
    "skipLibCheck": true
  }
}
